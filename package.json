    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "prism-svelte": "^0.4.7",
    "prismjs": "^1.26.0",
    "reading-time": "^1.5.0",
    "redaxios": "^0.4.1",
    "svelte": "3.45.0",
    "svelte-awesome": "^2.4.2",
    "svelte-check": "^2.2.11",
    "svelte-click-outside": "^1.0.0",
    "svelte-forms-lib": "^2.0.1",
    "svelte-jester": "^2.1.5",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.12",
    "ts-jest": "^27.1.2",
    "tslib": "^2.3.1",
